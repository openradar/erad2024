nProcessing time: 50.95 seconds\n"}],"key":"uDA5b8KIVf"}],"key":"rhjVgInrAd"},{"type":"block","kind":"notebook-content","data":{},"children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"Mental note: repeat once!","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"g3im94Er5a"}],"key":"wuRrj0CH4n"}],"key":"e5MyuiwwTW"},{"type":"block","kind":"notebook-content","data":{},"children":[{"type":"heading","depth":2,"position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"Multiprocess the generator","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"Xl39PVNoE5"}],"identifier":"multiprocess-the-generator","label":"Multiprocess the generator","html_id":"multiprocess-the-generator","implicit":true,"key":"ux9h0yo2QQ"}],"key":"UsfPZQ5ZoC"},{"type":"block","kind":"notebook-code","data":{"execution":{"iopub.execute_input":"2024-09-05T03:34:49.170555Z","iopub.status.busy":"2024-09-05T03:34:49.169935Z","iopub.status.idle":"2024-09-05T03:34:49.173709Z","shell.execute_reply":"2024-09-05T03:34:49.173274Z"}},"children":[{"type":"code","lang":"python","executable":true,"value":"# Both input and output are a list of file strings\ndef multi_generate(fstrs, procs=None):\n    pool = multiprocessing.Pool(\n        procs\n    )  # Pool of processors. Defaults to all available logical cores\n\n    results = []\n    # chunksize=1 means feed a process a new job as soon as the process is idle.\n    # In our case, this restricts the queue to one \"dispatcher\" which is faster.\n    r = pool.map_async(generate, fstrs, chunksize=1, callback=results.append)\n    r.wait()\n\n    return results[0]","key":"PIhcoPYXWB"},{"type":"output","id":"W4VsYfIBrOmefbECmiUpZ","data":[],"key":"CoHPX2bpLn"}],"key":"XFoPXLLTR5"},{"type":"block","kind":"notebook-content","data":{},"children":[{"type":"heading","depth":2,"position":{"star