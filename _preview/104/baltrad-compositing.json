ilenames = glob.glob(\"data/se*.h5\")","key":"s21ocPO2eK"},{"type":"output","id":"9N2tfuH4yta4JruucBYmW","data":[],"key":"NNCVqJVh4V"}],"key":"Q07BbYlzwL"},{"type":"block","kind":"notebook-code","data":{"execution":{"iopub.execute_input":"2024-09-05T07:16:48.021739Z","iopub.status.busy":"2024-09-05T07:16:48.021493Z","iopub.status.idle":"2024-09-05T07:16:59.223635Z","shell.execute_reply":"2024-09-05T07:16:59.223111Z"}},"children":[{"type":"code","lang":"python","executable":true,"value":"# Run with all defaults to a pre-configured area that uses the Google Maps projection.\n# First two arguments are product date and time. These are taken from the last input file if not specified.\nbefore = time.time()\ncomp = generator.generate(None, None, area=\"swegmaps_2000\")\nafter = time.time()\n\nrio = _raveio.new()\nrio.object = comp\nrio.save(\"data/comp_pcappi1000m.h5\")\n\nprint(\"Compositing took %3.2f seconds\" % (after - before))","key":"KJWc9wjRjM"},{"type":"output","id":"p5Nn0NPHkmi8wvY-OvISw","data":[{"name":"stdout","output_type":"stream","text":"Compositing took 11.17 seconds\n"}],"key":"NxpuJ7JEto"}],"key":"ctQ6rVemXK"},{"type":"block","kind":"notebook-content","data":{},"children":[{"type":"heading","depth":2,"position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"Tweak the plotter from earlier exercises","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"gN3SuDsQMK"}],"identifier":"tweak-the-plotter-from-earlier-exercises","label":"Tweak the plotter from earlier exercises","html_id":"tweak-the-plotter-from-earlier-exercises","implicit":true,"key":"JzUjMioLQo"}],"key":"asSd17D8xk"},{"type":"block","kind":"notebook-code","data":{"execution":{"iopub.execute_input":"2024-09-05T07:16:59.225829Z","iopub.status.busy":"2024-09-05T07:16:59.225470Z","iopub.status.idle":"2024-09-05T07:16:59.232585Z","shell.execute_reply":"2024-09-05T07:16:59.232146Z"}},"children":[{"type":"code","lang":"python","executable":true,"value":"# Two color palettes, one used in GoogleMapsPlugin, and the other from RAVE\nfrom GmapColorMap import dbzh as dbzp\n\n\n# Convert a 768-list palette to a matplotlib colorlist\ndef make_colorlist(pal):\n    colorlist = []\n    for i in range(0, len(pal), 3):\n        colorlist.append([pal[i] / 255.0, pal[i + 1] / 255.0, pal[i + 2] / 255.0])\n    return colorlist\n\n\n# Convert lists to colormaps\ndbzcl = make_colorlist(dbzp)\n\n# Then create a simple plotter\nimport matplotlib.pyplot as plt\n\nStringType = type(\"\")\n\n\ndef plot(data, colorlist=dbzcl, title=\"Composite\"):\n    mini, maxi = data.shape.index(min(data.shape)), data.shape.index(max(data.shape))\n    figsize = (20, 16)  # if mini == 0 else (12,8)\n    fig = plt.figure(figsize=figsize)\n    plt.title(title)\n    clist = (\n        colorlist\n        if type(colorlist) == StringType\n        else matplotlib.colors.ListedColormap(colorlist)\n    )\n    plt.imshow(data, cmap=clist, clim=(0, 255))\n    plt.colorbar(shrink=float(data.shape[mini]) / data.shape[maxi])","key":"uv4jMGLWZR"},{"type":"output","id":"a7hVwlxj4wzEbWI7xMnRu","data":[],"key":"xQxKQAOgQ4"}],"key":"JKkNIHUWfp"},{"type":"block","kind":"notebook-code","data":{"execution":{"iopub.execute_input":"2024-09-05T07:16:59.234514Z","iopub.status.busy":"2024-09-05T07:16:59.234173Z","iopub.status.idle":"2024-09-05T07:16:59.681704Z","shell.execute_reply":"2024-09-05T07:16:59.681067Z"}},"children":[{"type":"code","lang":"python","executable":true,"value":"plot(\n    comp.getParameter(\"DBZH\").getData(),\n    title=\"Default composite: DBZH 1000 m Pseudo-CAPPI, nearest radar\",\n)","key":"sBPQPnTD7J"},{"type":"output","id":"8jCMYM_XZAJaJeDz8efCs","data":[{"output_type":"display_data","metadata":{},"data":{"image/png":{"content_type":"image/png","hash":"14a1907a7fc7bbc7097bf4cad6bfe855","path":"/erad2024/_preview/104/build/14a1907a7fc7bbc7097bf4cad6bfe855.png"},"text/plain":{"content":"<Figure size 2000x1600 with 2 Axes>","cont