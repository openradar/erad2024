sy":"2024-09-05T07:15:21.745333Z","iopub.status.idle":"2024-09-05T07:15:22.107666Z","shell.execute_reply":"2024-09-05T07:15:22.107204Z"}},"children":[{"type":"code","lang":"python","executable":true,"value":"import _raveio\nimport ropo_realtime, ec_drqc\nimport matplotlib\nimport matplotlib.pyplot as plt\nimport numpy as np\nimport GmapColorMap","key":"fxBkvbtxAz"},{"type":"output","id":"CC1xXQsJUjP7YQmNt81sm","data":[],"key":"xdk0Jxkugw"}],"key":"wHaOq3i8Du"},{"type":"block","kind":"notebook-content","data":{},"children":[{"type":"heading","depth":3,"position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"Block of look-ups for display","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"CmS7N3Ayij"}],"identifier":"block-of-look-ups-for-display","label":"Block of look-ups for display","html_id":"block-of-look-ups-for-display","implicit":true,"key":"Kr3SaWuFsQ"}],"key":"sI2KCKsB9t"},{"type":"block","kind":"notebook-code","data":{"execution":{"iopub.execute_input":"2024-09-05T07:15:22.110151Z","iopub.status.busy":"2024-09-05T07:15:22.109913Z","iopub.status.idle":"2024-09-05T07:15:22.114255Z","shell.execute_reply":"2024-09-05T07:15:22.113832Z"}},"children":[{"type":"code","lang":"python","executable":true,"value":"PALETTE = {}  # To be populated\n\nUNDETECT = {\n    \"TH\": GmapColorMap.PUREWHITE,\n    \"DBZH\": GmapColorMap.PUREWHITE,\n    \"DR\": GmapColorMap.PUREWHITE,\n    \"VRADH\": GmapColorMap.GREY5,\n    \"RHOHV\": GmapColorMap.PUREWHITE,\n    \"ZDR\": GmapColorMap.PUREWHITE,\n}\n\nNODATA = {\n    \"TH\": GmapColorMap.WEBSAFEGREY,\n    \"DBZH\": GmapColorMap.WEBSAFEGREY,\n    \"DR\": GmapColorMap.WEBSAFEGREY,\n    \"VRADH\": GmapColorMap.GREY8,\n    \"RHOHV\": GmapColorMap.WEBSAFEGREY,\n    \"ZDR\": GmapColorMap.WEBSAFEGREY,\n}\n\nLEGEND = {\n    \"TH\": \"Radar reflectivity factor (dBZ)\",\n    \"DBZH\": \"Radar reflectivity factor (dBZ)\",\n    \"DR\": \"Depolarization ratio (dB)\",\n    \"VRADH\": \"Radial wind velocity away from radar (m/s)\",\n    \"RHOHV\": \"Cross-polar correlation coefficient\",\n    \"ZDR\": \"Differential reflectivity (dB)\",\n}\n\nTICKS = {\n    \"TH\": range(-30, 80, 10),\n    \"DBZH\": range(-30, 80, 10),\n    \"ZDR\": range(-8, 9, 2),\n    \"RHOHV\": np.arange(0, 11, 1) / 10.0,\n    \"VRADH\": range(-48, 56, 8),\n    \"DR\": range(-36, 3, 3),\n}","key":"LrjNrvaWLu"},{"type":"output","id":"-ccbtAwEKZFD4lFs2L_4O","data":[],"key":"ygV18KRfOK"}],"key":"lMpSDxjm61"},{"type":"block","kind":"notebook-content","data":{},"children":[{"type":"heading","depth":3,"position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"Colormap loader and loads","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"VTzZPJTinS"}],"identifier":"colormap-loader-and-loads","label":"Colormap loader and loads","html_id":"colormap-loader-and-loads","implicit":true,"key":"bLzHHToQA8"}],"key":"MrxjJCPtG6"},{"type":"block","kind":"notebook-code","data":{"execution":{"iopub.execute_input":"2024-09-05T07:15:22.116041Z","iopub.status.busy":"2024-09-05T07:15:22.115856Z","iopub.status.idle":"2024-09-05T07:15:22.120905Z","shell.execute_reply":"2024-09-05T07:15:22.120528Z"}},"children":[{"type":"code","lang":"python","executable":true,"value":"def loadPal(fstr, reverse=True):\n    fd = open(fstr)\n    LINES = fd.readlines()\n    fd.close()\n    pal = []\n    for line in LINES:\n        s = line.split()\n        if reverse:\n            s.reverse()\n        for val in s:\n            pal.append(int(float(val) * 255))\n    if reverse:\n        pal.reverse()\n    return pal\n\n\n# Colour maps by Fabio Crameri, http://www.fabiocrameri.ch/colourmaps.php, a couple of them modified\n# Todo: maybe use new cmweather colormaps\nPALETTE[\"DBZH\"] = loadPal(\"data/hawaii.txt\")\nPALETTE[\"DR\"] = loadPal(\"data/moleron.txt\", False)  # Modified oleron\nPALETTE[\"ZDR\"] = loadPal(\"data/oleron.txt\", False)\nPALETTE[\"RHOHV\"] = loadPal(\"data/mroma.txt\")  # Modified roma\nPALETTE[\"VRADH\"] = loadPal(\"data/vik.txt\", False)","key":"CUlvJhU5Xz"},{"type":"output","id":"SV4E2vud5ct-Ec4TgcB_V","data":[],"key":"yL8OPcHono"}],"key":"DXILmUbD7x"},{"type":"block","kind":"notebook-content","data":{},"children":[{"type":"heading","depth":3,"position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"Set up the display","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"r7l26y6f3c"}],"identifier":"set-up-the-display","label":"Set up the display","html_id":"set-up-the-display","implicit":true,"key":"i6GbT2AZvP"}],"key":"UO0h6TPfRO"},{"type":"block","kind":"notebook-code","data":{"execution":{"iopub.execute_input":"2024-09-05T07:15:22.122578Z","iopub.status.busy":"2024-09-05T07:15:22.122419Z","iopub.status.idle":"2024-09-05T07:15:22.127724Z","shell.execute_reply":"2024-09-05T07:15:22.127310Z"}},"children":[{"type":"code","lang":"python","executable":true,"value":"def plot(obj):\n    fig = plt.figure()\n    default_size = fig.get_size_inches()\n    fig.set_size_inches((default_size[0] * 2, default_size[1] * 2))\n\n    paramname = obj.getParameterNames()[0]\n    pal = PALETTE[paramname]\n    pal[0], pal[1], pal[2] = UNDETECT[\n        paramname\n    ]  # Special value - areas radiated but void of echo\n    pal[767], pal[766], pal[765] = NODATA[paramname]  # Special value - areas unradiated\n    if paramname == \"VRADH\":\n        pal[379], pal[380], pal[381] = GmapColorMap.PUREWHITE  # VRADH isodop\n        pal[382], pal[383], pal[384] = GmapColorMap.PUREWHITE  # VRADH isodop\n        pal[385], pal[386], pal[387] = GmapColorMap.PUREWHITE  # VRADH isodop\n    colorlist = []\n    for i in range(0, len(pal), 3):\n        colorlist.append([pal[i] / 255.0, pal[i + 1] / 255.0, pal[i + 2] / 255.0])\n\n    param = obj.getParameter(paramname)\n    data = param.getData()\n    data = data * param.gain + param.offset\n\n    im = plt.imshow(data, cmap=matplotlib.colors.ListedColormap(colorlist))\n    cax = plt.gca()\n    cax.axes.get_xaxis().set_visible(False)\n    cax.axes.get_yaxis().set_visible(False)\n\n    cb = plt.colorbar(ticks=TICKS[paramname])\n    cb.set_label(LEGEND[paramname])\n\n    plt.show()","key":"jZxPAfBFuU"},{"type":"output","id":"my56wderAaXjdcG6rgH9E","data":[],"key":"cSfrnKvuVw"}],"key":"beyrZ3iGtH"},{"type":"block","kind":"notebook-content","data":{},"children":[{"type":"heading","depth":2,"position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"Do the science","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"fpCNUiZGmF"}],"identifier":"do-the-science","label":"Do the science","html_id":"do-the-science","implicit":true,"key":"FzWFl3Qi5v"},{"type":"heading","depth":3,"position":{"start":{"line":2,"column":1},"end":{"line":2,"column":1}},"children":[{"type":"text","value":"Read the polar volume, QC the reflectivity using legacy ROPO, and then save the QC:ed result","position":{"start":{"line":2,"column":1},"end":{"line":2,"column":1}},"key":"ptrXoCfGRv"}],"identifier":"read-the-polar-volume-qc-the-reflectivity-using-legacy-ropo-and-then-save-the-qc-ed-result","label":"Read the polar volume, QC the reflectivity using legacy ROPO, and then save the QC:ed result","html_id":"read-the-polar-volume-qc-the-reflectivity-using-legacy-ropo-and-then-save-the-qc-ed-result","implicit":true,"key":"k0jwwgTvhG"}],"key":"LynkZHP5ih"},{"type":"block","kind":"notebook-code","data":{"execution":{"iopub.execute_input":"2024-09-05T07:15:22.129619Z","iopub.status.busy":"2024-09-05T07:15:22.129222Z","iopub.status.idle":"2024-09-05T07:15:26.124089Z","shell.execute_reply":"2024-09-05T07:15:26.123528Z"}},"children":[{"type":"code","lang":"python","executable":true,"value":"rio = _raveio.open(\"data/2019051509_00_ODIMH5_PVOL6S_VOL_casra.16.h5\")\nrio.object = ropo_realtime.generate(rio.object)\nrio.save(\"