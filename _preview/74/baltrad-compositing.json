pColorMap import dbzh as dbzp\n\n\n# Convert a 768-list palette to a matplotlib colorlist\ndef make_colorlist(pal):\n    colorlist = []\n    for i in range(0, len(pal), 3):\n        colorlist.append([pal[i] / 255.0, pal[i + 1] / 255.0, pal[i + 2] / 255.0])\n    return colorlist\n\n\n# Convert lists to colormaps\ndbzcl = make_colorlist(dbzp)\n\n# Then create a simple plotter\nimport matplotlib.pyplot as plt\n\nStringType = type(\"\")\n\n\ndef plot(data, colorlist=dbzcl, title=\"Composite\"):\n    mini, maxi = data.shape.index(min(data.shape)), data.shape.index(max(data.shape))\n    figsize = (20, 16)  # if mini == 0 else (12,8)\n    fig = plt.figure(figsize=figsize)\n    plt.title(title)\n    clist = (\n        colorlist\n        if type(colorlist) == StringType\n        else matplotlib.colors.ListedColormap(colorlist)\n    )\n    plt.imshow(data, cmap=clist, clim=(0, 255))\n    plt.colorbar(shrink=float(data.shape[mini]) / data.shape[maxi])","key":"KJcUsHoATp"},{"type":"output","id":"Lfdutytslo6tJ0bRPDhW2","data":[],"key":"cji6xlYTvT"}],"key":"wDrNVY85Bn"},{"type":"block","kind":"notebook-code","data":{"execution":{"iopub.execute_input":"2024-08-15T07:22:26.653735Z","iopub.status.busy":"2024-08-15T07:22:26.653309Z","iopub.status.idle":"2024-08-15T07:22:27.154803Z","shell.execute_reply":"2024-08-15T07:22:27.154112Z"}},"children":[{"type":"code","lang":"python","executable":true,"value":"plot(\n    comp.getParameter(\"DBZH\").getData(),\n    title=\"Default composite: DBZH 1000 m Pseudo-CAPPI, nearest radar\",\n)","key":"csfXNm8RlA"},{"type":"output","id":"hQdMbC-odABlQvUPjGLs-","data":[{"output_type":"display_data","metadata":{},"data":{"image/png":{"content_type":"image/png","hash":"14a1907a7fc7bbc7097bf4cad6bfe855","path":"/erad2024/_preview/74/build/14a1907a7fc7bbc7097bf4cad6bfe855.png"},"text/plain":{"content":"<Figure size 2000x1600 with 2 Axes>","content_type":"text/plain"}}}],"key":"S0wzWLMI29"}],"key":"CDKXHcy4y3"},{"type":"block","kind":"notebook-content","d