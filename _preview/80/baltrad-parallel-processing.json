y":"DAwLYSOxWK"},{"type":"block","kind":"notebook-content","data":{},"children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"Yay! Now we’re going to set up some rudimentary functionality that will allow us to distribute a processing load among our cores.","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"be9DX5q4bp"}],"key":"fmdM9acld6"}],"key":"YlJj1syNbs"},{"type":"block","kind":"notebook-content","data":{},"children":[{"type":"heading","depth":2,"position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"Define a generator","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"LzKQFaFrw0"}],"identifier":"define-a-generator","label":"Define a generator","html_id":"define-a-generator","implicit":true,"key":"HCFMrO2We3"}],"key":"T0y9NtfS1p"},{"type":"block","kind":"notebook-code","data":{"execution":{"iopub.execute_input":"2024-08-20T05:46:53.276125Z","iopub.status.busy":"2024-08-20T05:46:53.275799Z","iopub.status.idle":"2024-08-20T05:46:53.375068Z","shell.execute_reply":"2024-08-20T05:46:53.374560Z"}},"children":[{"type":"code","lang":"python","executable":true,"value":"import os\nimport _raveio, odc_polarQC\n\n# Specify the processing chain\nodc_polarQC.algorithm_ids = [\n    \"ropo\",\n    \"beamb\",\n    \"radvol-att\",\n    \"radvol-broad\",\n    \"rave-overshooting\",\n    \"qi-total\",\n]\n\n\n# Run processing chain on a single file. Return an output file string.\ndef generate(file_string):\n    rio = _raveio.open(file_string)\n\n    pvol = rio.object\n    pvol = odc_polarQC.QC(pvol)\n    rio.object = pvol\n\n    # Derive an output file name\n    path, fstr = os.path.split(file_string)\n    ofstr = os.path.join(path, \"qc_\" + fstr)\n\n    rio.save(ofstr)\n    return ofstr","key":"oFb9zSfHH3"},{"type":"output","id":"yuCtcDpWP3Tkya8OvmdX_","data":[],"key":"ZTUw3aSci8"}],"key":"XtiPjm0x7m"},{"type":"block","kind":"notebook-content","data":{},"children":[{"type":"heading","depth":2,"position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"Feed the generator, sequentially","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"QoGwIfm8LK"}],"identifier":"feed-the-generator-sequentially","label":"Feed the generator, sequentially","html_id":"feed-the-generator-sequentially","implicit":true,"key":"ZRAIs4FECt"}],"key":"Tp9WXmhnWH"},{"type":"block","kind":"notebook-code","data":{"execution":{"iopub.execute_input":"2024-08-20T05:46:53.377576Z","iopub.status.busy":"2024-08-20T05:46:53.377320Z","iopub.status.idle":"2024-08-20T05:47:44.611057Z","shell.execute_reply":"2024-08-20T05:47:44.610533Z"}},"children":[{"type":"code","lang":"python","executable":true,"value":"import glob, time\n\nifstrs = glob.glob(\"data/se*.h5\")\nbefore = time.time()\nfor fstr in ifstrs:\n    print(fstr, generate(fstr))\nafter = time.time()\n\nprint(\"Processing time: %3.2f seconds\" % (after - before))","key":"TU0E9Vxtoo"},{"type":"output","id":"hyEcWWPTaeafl4rksNGqX","data":[{"name":"stdout","output_type":"stream","text":"data/sease.h5 data/qc_sease.h5\n"},{"name":"stdout","output_type":"stream","text":"data/sehud.h5 data/qc_sehud.h5\n"},{"name":"stdout","output_type":"stream","text":"data/searl.h5 data/qc_searl.h5\n"},{"name":"stdout","output_type":"stream","text":"data/sevar.h5 data/qc_sevar.h5\n"},{"name":"stdout","output_type":"stream","text":"data/sevil.h5 data/qc_sevil.h5\n"},{"name":"stdout","output_type":"stream","text":"data/selul.h5 data/qc_selul.h5\n"},{"name":"stdout","output_type":"stream","text":"data/seang.h5 data/qc_seang.h5\n"},{"name":"stdout","output_type":"stream","text":"data/sekkr.h5 data/qc_sekkr.h5\n"},{"name":"stdout","output_type":"stream","text":"data/seosu.h5 data/qc_seosu.h5\n"},{"name":"stdout","output_type":"stream","text":"data/sekir.h5 data/qc_sekir.h5\n"},{"name":"stdout"