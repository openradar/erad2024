tent","data":{},"children":[{"type":"heading","depth":2,"position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"Create a simple plotter for B-scans, elaborating the example from the I/O exercise","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"R9hsB0obWe"}],"identifier":"create-a-simple-plotter-for-b-scans-elaborating-the-example-from-the-i-o-exercise","label":"Create a simple plotter for B-scans, elaborating the example from the I/O exercise","html_id":"create-a-simple-plotter-for-b-scans-elaborating-the-example-from-the-i-o-exercise","implicit":true,"key":"d0BgdU5omR"}],"key":"bT1dHBJzel"},{"type":"block","kind":"notebook-code","data":{"execution":{"iopub.execute_input":"2024-08-28T06:07:23.522105Z","iopub.status.busy":"2024-08-28T06:07:23.521777Z","iopub.status.idle":"2024-08-28T06:07:23.540625Z","shell.execute_reply":"2024-08-28T06:07:23.540109Z"}},"children":[{"type":"code","lang":"python","executable":true,"value":"# Two color palettes, one used in GoogleMapsPlugin, and the other from RAVE\nfrom GmapColorMap import dbzh as dbzp\nfrom rave_win_colors import continuous_MS as vradp\n\n\n# Convert a 768-list palette to a matplotlib colorlist\ndef make_colorlist(pal):\n    colorlist = []\n    for i in range(0, len(pal), 3):\n        colorlist.append([pal[i] / 255.0, pal[i + 1] / 255.0, pal[i + 2] / 255.0])\n    return colorlist\n\n\n# Convert lists to colormaps\ndbzcl = make_colorlist(dbzp)\nvradcl = make_colorlist(vradp)\n\n# Then create a simple plotter\nimport matplotlib.pyplot as plt\n\n# from types import StringType\nStringType = type(\"\")\n\n\ndef plot(data, colorlist=dbzcl, title=\"B-scan\"):\n    mini, maxi = data.shape.index(min(data.shape)), data.shape.index(max(data.shape))\n    figsize = (16, 12) if mini == 0 else (12, 8)\n    fig = plt.figure(figsize=figsize)\n    plt.title(title)\n    clist = (\n        colorlist\n        if type(colorlist) == StringType\n        else matplotlib.colors.ListedColormap(colorlist)\n    )\n    plt.imshow(data, cmap=clist, clim=(0, 255))\n    plt.colorbar(shrink=float(data.shape[mini]) / data.shape[maxi])","key":"LPPacDK7ny"},{"type":"output","id":"2hWliRyqw8len_2yhQpVH","data":[],"key":"h7z2mRKecL"}],"key":"OCY6ASXVDW"},{"type":"block","kind":"notebook-content","data":{},"children":[{"type":"heading","depth":2,"position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"Access the polar volume and plot VRAD data from the lowest scan","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"jekQcptGaj"}],"identifier":"access-the-polar-volume-and-plot-vrad-data-from-the-lowest-scan","label":"Access the polar volume and plot VRAD data from the lowest scan","html_id":"access-the-polar-volume-and-plot-vrad-data-from-the-lowest-scan","implicit":true,"key":"iFRLrXwIyT"}],"key":"b93kmNhb4h"},{"type":"block","kind":"notebook-code","data":{"execution":{"iopub.execute_input":"2024-08-28T06:07:23.542889Z","iopub.status.busy":"2024-08-28T06:07:23.542428Z","iopub.status.idle":"2024-08-28T06:07:23.876291Z","shell.execute_reply":"2024-08-28T06:07:23.875636Z"}},"children":[{"type":"code","lang":"python","executable":true,"value":"pvol = rio.object\nplot(pvol.getScan(0).getParameter(\"VRADH\").getData(), vradcl, \"Original VRAD\")","key":"G1obkPOC7R"},{"type":"output","id":"dPtjQiWH418Su9Xs1w8Sj","data":[{"output_type":"display_data","metadata":{},"data":{"image/png":{"content_type":"image/png","hash":"620731350ddcf40a5de2dcd414f319f2","path":"/erad2024/_preview/96/build/620731350ddcf40a5de2dcd414f319f2.png"},"text/plain":{"content":"<Figure size 1600x1200 with 2 Axes>","content_type":"text/plain"}}}],"key":"ai0GjhbiqJ"}],"key":"kocTdF2Ntw"},{"type":"block","kind":"notebook-content","data":{},"children":[{"type":"heading","depth":2,"position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"Dealias the volum