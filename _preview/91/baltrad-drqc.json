e":"import _raveio\nimport ropo_realtime, ec_drqc\nimport matplotlib\nimport matplotlib.pyplot as plt\nimport numpy as np\nimport GmapColorMap","key":"ANdmshoth5"},{"type":"output","id":"SlObffbSgaaNtKzkPpfCt","data":[],"key":"hDJLXdC8cv"}],"key":"vjrwKuhtdj"},{"type":"block","kind":"notebook-content","data":{},"children":[{"type":"heading","depth":3,"position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"Block of look-ups for display","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"AFXpdFAfP6"}],"identifier":"block-of-look-ups-for-display","label":"Block of look-ups for display","html_id":"block-of-look-ups-for-display","implicit":true,"key":"aUZ0s4Lcf0"}],"key":"drxpl1Gk44"},{"type":"block","kind":"notebook-code","data":{"execution":{"iopub.execute_input":"2024-08-26T16:08:57.253282Z","iopub.status.busy":"2024-08-26T16:08:57.252904Z","iopub.status.idle":"2024-08-26T16:08:57.257242Z","shell.execute_reply":"2024-08-26T16:08:57.256750Z"}},"children":[{"type":"code","lang":"python","executable":true,"value":"PALETTE = {}  # To be populated\n\nUNDETECT = {\n    \"TH\": GmapColorMap.PUREWHITE,\n    \"DBZH\": GmapColorMap.PUREWHITE,\n    \"DR\": GmapColorMap.PUREWHITE,\n    \"VRADH\": GmapColorMap.GREY5,\n    \"RHOHV\": GmapColorMap.PUREWHITE,\n    \"ZDR\": GmapColorMap.PUREWHITE,\n}\n\nNODATA = {\n    \"TH\": GmapColorMap.WEBSAFEGREY,\n    \"DBZH\": GmapColorMap.WEBSAFEGREY,\n    \"DR\": GmapColorMap.WEBSAFEGREY,\n    \"VRADH\": GmapColorMap.GREY8,\n    \"RHOHV\": GmapColorMap.WEBSAFEGREY,\n    \"ZDR\": GmapColorMap.WEBSAFEGREY,\n}\n\nLEGEND = {\n    \"TH\": \"Radar reflectivity factor (dBZ)\",\n    \"DBZH\": \"Radar reflectivity factor (dBZ)\",\n    \"DR\": \"Depolarization ratio (dB)\",\n    \"VRADH\": \"Radial wind velocity away from radar (m/s)\",\n    \"RHOHV\": \"Cross-polar correlation coefficient\",\n    \"ZDR\": \"Differential reflectivity (dB)\",\n}\n\nTICKS = {\n    \"TH\": range(-30, 80, 10),\n    \"DBZH\": range(-30, 80, 10),\n    \"ZDR\": range(-8, 9, 2),\n    \"RHOHV\": np.arange(0, 11, 1) / 10.0,\n    \"VRADH\": range(-48, 56, 8),\n    \"DR\": range(-36, 3, 3),\n}","key":"ANAUExHmlV"},{"type":"output","id":"JTub6L6CeFmpzksJnCeeq","data":[],"key":"TvXxFYKKJB"}],"key":"cRRhVwkppb"},{"type":"block","kind":"notebook-content","data":{},"children":[{"type":"heading","depth":3,"position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"Colormap loader and loads","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"nlE9x4K0g7"}],"identifier":"colormap-loader-and-loads","label":"Colormap loader and loads","html_id":"colormap-loader-and-loads","implicit":true,"key":"U8IgFatoE6"}],"key":"jcLpFNYuEE"},{"type":"block","kind":"notebook-code","data":{"execution":{"iopub.execute_input":"2024-08-26T16:08:57.259331Z","iopub.status.busy":"2024-08-26T16:08:57.258954Z","iopub.status.idle":"2024-08-26T16:08:57.264023Z","shell.execute_reply":"2024-08-26T16:08:57.263513Z"}},"children":[{"type":"code","lang":"python","executable":true,"value":"def loadPal(fstr, reverse=True):\n    fd = open(fstr)\n    LINES = fd.readlines()\n    fd.close()\n    pal = []\n    for line in LINES:\n        s = line.split()\n        if reverse:\n            s.reverse()\n        for val in s:\n            pal.append(int(float(val) * 255))\n    if reverse:\n        pal.reverse()\n    return pal\n\n\n# Colour maps by Fabio Crameri, http://www.fabiocrameri.ch/colourmaps.php, a couple of them modified\n# Todo: maybe use new cmweather colormaps\nPALETTE[\"DBZH\"] = loadPal(\"data/hawaii.txt\")\nPALETTE[\"DR\"] = loadPal(\"data/moleron.txt\", False)  # Modified oleron\nPALETTE[\"ZDR\"] = loadPal(\"data/oleron.txt\", False)\nPALETTE[\"RHOHV\"] = loadPal(\"data/mroma.txt\")  # Modified roma\nPALETTE[\"VRADH\"] = loadPal(\"data/vik.txt\", False)","key":"DdQwZkSJn8"},{"type":"output","id":"5lmwSk0Ur6xThVFjLhJEZ","data":[],"key":"WGQslGPetz"}],"key":"BcYQoQ2gyN"},{"type":"block","kind":"notebook-content","data":{},"children":[{"type":"heading","depth":3,"position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"Set up the display","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"V2UnWMlMT8"}],"identifier":"set-up-the-display","label":"Set up the display","html_id":"set-up-the-display","implicit":true,"key":"IVI339Smsy"}],"key":"WEiYZaR2yZ"},{"type":"block","kind":"notebook-code","data":{"execution":{"iopub.execute_input":"2024-08-26T16:08:57.266047Z","iopub.status.busy":"2024-08-26T16:08:57.265735Z","iopub.status.idle":"2024-08-26T16:08:57.270829Z","shell.execute_reply":"2024-08-26T16:08:57.270385Z"}},"children":[{"type":"code","lang":"python","executable":true,"value":"def plot(obj):\n    fig = plt.figure()\n    default_size = fig.get_size_inches()\n    fig.set_size_inches((default_size[0] * 2, default_size[1] * 2))\n\n    paramname = obj.getParameterNames()[0]\n    pal = PALETTE[paramname]\n    pal[0], pal[1], pal[2] = UNDETECT[\n        paramname\n    ]  # Special value - areas radiated but void of echo\n    pal[767], pal[766], pal[765] = NODATA[paramname]  # Special value - areas unradiated\n    if paramname == \"VRADH\":\n        pal[379], pal[380], pal[381] = GmapColorMap.PUREWHITE  # VRADH isodop\n        pal[382], pal[383], pal[384] = GmapColorMap.PUREWHITE  # VRADH isodop\n        pal[385], pal[386], pal[387] = GmapColorMap.PUREWHITE  # VRADH isodop\n    colorlist = []\n    for i in range(0, len(pal), 3):\n        colorlist.append([pal[i] / 255.0, pal[i + 1] / 255.0, pal[i + 2] / 255.0])\n\n    param = obj.getParameter(paramname)\n    data = param.getData()\n    data = data * param.gain + param.offset\n\n    im = plt.imshow(data, cmap=matplotlib.colors.ListedColormap(colorlist))\n    cax = plt.gca()\n    cax.axes.get_xaxis().set_visible(False)\n    cax.axes.get_yaxis().set_visible(False)\n\n    cb = plt.colorbar(ticks=TICKS[paramname])\n    cb.set_label(LEGEND[paramname])\n\n    plt.show()","key":"KDu3rNmFDN"},{"type":"output","id":"Zs9ZP8ADIeqfShAvGdFx1","data":[],"key":"PkLf2ZJ9c0"}],"key":"SucC0fAF5a"},{"type":"block","kind":"notebook-content","data":{},"children":[{"type":"heading","depth":2,"position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"Do the science","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"AG1vW8ywMk"}],"identifier":"do-the-science","label":"Do the science","html_id":"do-the-science","implicit":true,"key":"MduGNJvyR5"},{"type":"heading","depth":3,"position":{"start":{"line":2,"column":1},"end":{"line":2,"column":1}},"children":[{"type":"text","value":"Read the polar volume, QC the reflectivity using legacy ROPO, and then save the QC:ed result","position":{"start":{"line":2,"column":1},"end":{"line":2,"column":1}},"key":"U0oTeqq3rf"}],"identifier":"read-the-polar-volume-qc-the-reflectivity-using-legacy-ropo-and-then-save-the-qc-ed-result","label":"Read the polar volume, QC the reflectivity using legacy ROPO, and then save the QC:ed result","html_id":"read-the-polar-volume-qc-the-reflectivity-using-legacy-ropo-and-then-save-the-qc-ed-result","implicit":true,"key":"lOfxcob1Zb"}],"key":"fkIZreOVBE"},{"type":"block","kind":"notebook-code","data":{"execution":{"iopub.execute_input":"2024-08-26T16:08:57.272765Z","iopub.status.busy":"2024-08-26T16:08:57.272449Z","iopub.status.idle":"2024-08-26T16:09:01.082892Z","shell.execute_reply":"2024-08-26T16:09:01.082383Z"}},"children":[{"type":"code","lang":"python","executable":true,"value":"rio = _raveio.open(\"data/2019051509_00_ODIMH5_PVOL6S_VOL_casra.16.h5\")\nrio.object = ropo_realtime.generate(rio.object)\nrio.save(\"data/2019051509_00_ODIMH5_PVOL6S_VOL_casra.ropo.h5\")","key":"PZzYxLHlq3"},{"type":"output","id":"cSUT4uMij3As8CU2Gu7eE","data":[],"key":"mVXDa6oGg4"}],"key":"lY4aBhkMee"},{"type":"block","kind":"notebook